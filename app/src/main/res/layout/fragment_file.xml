<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fab="http://schemas.android.com/apk/res-auto">

    <com.example.baeksubuntu.deepstreaming.Custom_WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/fragment_file_webview_id">

    </com.example.baeksubuntu.deepstreaming.Custom_WebView>

    <com.melnykov.fab.FloatingActionButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/fragment_file_folder_btn_id"
        android:layout_gravity="bottom|right"
        android:layout_margin="16dp"
        android:src="@drawable/ic_folder_open_black_24dp"
        fab:fab_colorNormal="@color/fabPrimary"
        fab:fab_colorPressed="@color/fabPressed"
        fab:fab_colorRipple="@color/fabRipple"/>

</FrameLayout>
